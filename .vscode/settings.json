{
    "workbench.colorCustomizations": {
        "activityBar.background": "#591322",
        "titleBar.activeBackground": "#7C1B30",
        "titleBar.activeForeground": "#FEFBFC"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "build/" : true
    },
    "files.associations": {
        "*.py": "python",
        "*.tcc": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "string_view": "cpp",
        "random": "cpp",
        "array": "cpp",
        "initializer_list": "cpp"
    }
}
